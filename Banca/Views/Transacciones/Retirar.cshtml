
@using Banca.Entities
@{
  ViewData["Title"] = "Retirar";
}
Balance de la cuenta
<h2>@ViewBag.Ahorro.Balance.ToString("c")</h2>


@if (!string.IsNullOrEmpty(ViewBag.Error))
{
    <div class="text-danger">
        @ViewBag.Error
    </div>
}

@await Html.PartialAsync("_Transaccion", new Transaccion{
    AhorroId = ViewBag.Ahorro.Id,
    Tipo = Tipo.Retiro,
    FechaDeRegistro = DateTime.Now
})